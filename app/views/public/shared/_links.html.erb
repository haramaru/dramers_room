<%- if controller_name != 'sessions' %>
  <div>
    <%= link_to "ログイン", new_session_path(resource_name) %><br />
  </div>

<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <div>
    <%= link_to "新規登録", new_registration_path(resource_name) %><br />
  </div>

<% end %>


